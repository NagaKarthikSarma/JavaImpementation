package com.karthik.core.collections.threading;

public class RunnableThre implements Runnable{

	public static void main(String[] args) {
	

	}

	@Override
	public void run() {
		int i=0;
		for(i=0;i<10;i++) {
			System.out.println("the value of i: "+ i +"--"+Thread.currentThread().getName());
		}
		
	}

}
