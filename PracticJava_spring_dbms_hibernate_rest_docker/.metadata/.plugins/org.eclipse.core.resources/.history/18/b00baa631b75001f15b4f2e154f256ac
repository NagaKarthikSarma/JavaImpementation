package com.karthik.core.collections.threading;

public class RaceConditions implements Runnable {

	public static void main(String[] args) {
		
		//synchronized method
		//synchronized block
		
   Thread t1 = new Thread(new RaceConditions());
   
		
		

	}

	@Override
	public void run() {
		synchronized{
		
		int a =0;
		for (a=0;a<1000;a++) {
			
			a=a+1;
		}
		}
		
		
	}

}
